/* pages/userCenter/userCenter.wxss */
.page{
    background-color: #F3F3F3;
    width: 100%;
    height: 100%;
}
.header{
    width: 100%;
    height: 260rpx;
    position: fixed;
    top:0;
    left:0;
}
.centerContent{
padding-top: 260rpx;
box-sizing: border-box;
}
.header image{
    width: 100%;
    height: 260rpx;
}
.header .headImg{
    width: 144rpx;
    height: 144rpx;
    border-radius: 50%;
    background-color: #fff;
    position: absolute;
    top:47rpx;
    left: 31rpx;
}
.header .name{
    position: absolute;
    top:115rpx;
    left:205rpx;
    line-height: 39rpx;
}
.coupon{
    margin-top:10rpx;
    background-color: #fff;
}
 .coupon .couponIcon{
    width: 15rpx;
    height: 26rpx;
    margin-right: 55rpx;

}
.couponTitle{
    margin-top:30rpx;
    background-color: #fff;
    width: 100%;
    height: 107rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    border-bottom: 1rpx solid #d0d0d0;
    font-size: 33rpx;
    justify-content:space-between;
    flex-wrap: nowrap;
}
.couponTitle .couponLeft{
    display: flex;
    flex-direction:row;
    flex-flow: nowrap;
    align-content: center;
}
.couponTitle image{
    width: 50rpx;
    height: 51rpx;
    margin-left:31rpx;
}
 .myCoupon .youhui{
    width: 15rpx;
    height: 26rpx;
    margin-right: 55rpx;
}
.couponTitle text{
    font-size: 33rpx;
    margin-left: 25rpx;
}
.coupon-Item{
    width: 100%;
    height: 130rpx;
    border-bottom: 1rpx solid #d0d0d0;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-items: center;
    box-sizing: border-box;
}
.coupon-Item .couponImg{
    width: 124rpx;
    height: 80rpx;
    margin-left:-13rpx;
}
.coupon-Item .couponDate{
    color: #949494
}
.coupon-Item .couponText{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items:flex-start;
    font-size: 26rpx;
    margin-left:-66rpx;
}
.ifCoupon{
    font-size: 33rpx;
    width: 100%;
    text-align: center;
    padding: 30rpx 0;
    color: #ccc;

}
.arrow{
    width: 15rpx;
    height: 26rpx;
}
.buyHistory-Item{
    width: 100%;
    height: 130rpx;
    border-bottom: 1rpx solid #F3F3F3;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-items: center;
    box-sizing: border-box;
}
.buyHistory-Item .buyHistoryImg{
    width: 124rpx;
    height: 80rpx;
    margin-left:20rpx;
}
.buyHistoryText{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items:flex-start;
    margin-top:15rpx;
   
}
.buyHistory-Item .buyHistoryType{
    font-size: 26rpx;
}
.buyHistory-Item .receiveInfo{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    width: 450rpx;
    height: 31rpx;
}
.buyHistory-Item .receivepeople{
    color: #949494;
    font-size: 26rpx;
}
.buyHistory-Item .buyHistoryArrow{
    margin-right: 35rpx;
}
.price{
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
}
.rmb{
    width: 25rpx;
    height: 25rpx;
}
.rmbText{
    color: #D81E06;
    font-size: 31rpx;
}
.myOrder{
    margin-top:30rpx;
    background-color: #fff;
    width: 100%;
    height: 107rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    border-bottom: 1rpx solid #d0d0d0;
    font-size: 33rpx;
    justify-content:space-between;
    flex-wrap: nowrap ;

}
 .myOrder .myOrderArrow{
    width: 15rpx;
    height: 26rpx;
    margin-right: 55rpx;
    
}
.myOrder .myOrderLeft{
    display: flex;
    flex-direction:row;
    flex-flow: nowrap;
    align-content: center;
}
 .myOrder .myOrderImg{
    width: 50rpx;
    height: 50rpx;
    margin-left:31rpx;
    margin-right:31rpx;
}