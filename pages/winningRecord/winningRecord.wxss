page{
    background-color: #eee;
    width: 100%;
    height: 100%;
}
.page{
    display: flex;
    flex-direction: column;
    align-items: center;
}
.item{
    background-color: #fff;
    font-size: 28rpx;
    width: 90%;
    margin-top:28rpx;
    padding: 20rpx;
    box-sizing: border-box;
    border-radius: 10rpx;
}
view{
    margin:10rpx 0;
}
.title{
    font-size: 34rpx;
}
.c333{
    color: #333;
}
.order{
    width: 100%;
    margin-top:30rpx;
    font-size: 36rpx;
    color: #ccc;
    text-align: center;
}
.arrow{
    width: 16rpx;
    height: 26rpx;
    position: absolute;
    right: 40rpx;
    top:18rpx;

}
.deliveryNo{
    position: relative;
}